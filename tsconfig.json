{
  "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "lib": ["ES2020"],
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "outDir": "./dist",
  },
  "include": ["./src/"],
}